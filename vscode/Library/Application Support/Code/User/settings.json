{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    },
    "editor.formatOnSave": true,
    "editor.insertSpaces": true
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "application.shellEnvironmentResolutionTimeout": 30,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.guides.bracketPairs": "active",
  "editor.indentSize": "tabSize",
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.rulers": [
    80
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.autoReveal": "focusNoScroll",
  "explorer.compactFolders": false,
  "files.associations": {
    ".eslintignore": "plaintext"
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "go.alternateTools": {
    "gopls": "/Users/fjorn/.local/share/mise/installs/go/1.23.0/bin/gopls"
  },
  "go.gopath": "/Users/fjorn/.local/share/mise/installs/go/1.23.0/bin/go",
  "go.goroot": "/Users/fjorn/.local/share/mise/installs/go/1.23.0",
  "interactiveWindow.executeWithShiftEnter": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.creationMode": "perFile",
  "ltex.configurationTarget": {
    "dictionary": "userExternalFile",
    "disabledRules": "userExternalFile",
    "hiddenFalsePositives": "workspaceFolderExternalFile"
  },
  "ltex.dictionary": {
    "en-US": [
      ":~/dotfiles/ltex/.config/ltex/dictionary.en-US.txt"
    ]
  },
  "ltex.disabledRules": {
    "en-US": [
      ":~/dotfiles/ltex/.config/ltex/disabledRules.en-US.txt"
    ]
  },
  "ltex.enabled": [
    "latex",
    "markdown"
  ],
  "markdownlint.config": {
    "MD029": {
      "style": "one_or_ordered"
    }
  },
  "python.analysis.extraPaths": [
    "extrapath"
  ],
  "python.analysis.include": [
    "extrapath"
  ],
  "python.analysis.typeCheckingMode": "standard",
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.fontFamily": "'Monaspace Neon', monospace",
  "editor.fontLigatures": "'calt', 'liga'",
  "terminal.integrated.profiles.osx": {
    "bash": {
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "path": "bash"
    },
    "tmux": {
      "icon": "terminal-tmux",
      "path": "tmux"
    },
    "zsh": {
      "args": [
        "-l",
      ],
      "path": "zsh"
    }
  },
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "window.systemColorTheme": "auto",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Latte",
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.sideBar.location": "right",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "extensions.ignoreRecommendations": true,
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
}